{
  "name": "editor",
  "version": "1.0.0",
  "description": "",
  "main": "dist/electron.js",
  "scripts": {
    "preclean": "echo \"\\x1b[101m\\x1b[97m[ Cleaning ]\\x1b[0m\"",
    "clean": "(echo \"Cleaning...\") && (cmake-js -d ../engine clean) && (rm -rf ./dist || true) && (rm ../engine/frame.png || true)",
    "prebuild-electron": "echo \"\\x1b[102m\\x1b[97m[ Building Electron ]\\x1b[0m\"",
    "build-electron": "(echo \"Building Electron...\") && (tsc ./src/electron.ts) && (mv ./src/electron.js ./dist/electron.js)",
    "prebuild-react": "echo \"\\x1b[106m\\x1b[97m[ Building React ]\\x1b[0m\"",
    "build-react": "(echo \"Building React...\") && webpack --config ./webpack.config.js",
    "prebuild-engine": "echo \"\\x1b[103m\\x1b[97m[ Building Engine ]\\x1b[0m\"",
    "build-engine": "(echo \"Building Engine...\") && cmake-js -d ../engine compile",
    "prebuild-all": "echo \"\\x1b[104m\\x1b[97m[ Building All ]\\x1b[0m\"",
    "build-all": "yarn build-engine && yarn build-react && yarn build-electron",
    "prelaunch-electron": "echo \"\\x1b[105m\\x1b[97m[ Launching Electron ]\\x1b[0m\"",
    "launch-electron": "electron dist/electron.js",
    "preinstall": "yarn clean && yarn build-all",
    "prestart": "yarn",
    "start": "yarn launch-electron",
    "prestart-editor": "yarn build-react && yarn build-electron",
    "start-editor": "yarn launch-electron"
  },
  "author": "Deepak Ramalingam",
  "license": "ISC",
  "dependencies": {
    "fs": "^0.0.1-security",
    "node-addon-api": "^4.2.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "cmake-js": "^6.2.1",
    "electron": "^16.0.1",
    "html-webpack-plugin": "^5.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1"
  }
}
