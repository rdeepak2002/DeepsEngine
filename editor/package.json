{
  "name": "editor",
  "version": "1.0.0",
  "description": "",
  "main": "dist/electron.js",
  "scripts": {
    "preclean": "echo \"\\x1b[101m\\x1b[97m[ Cleaning ]\\x1b[0m\"",
    "clean": "(echo \"Cleaning...\") && (cmake-js -d ../engine clean) && (rm -rf ./dist || true) && (rm ../engine/frame.png || true)",
    "prebuild-react": "echo \"\\x1b[106m\\x1b[97m[ Building React ]\\x1b[0m\"",
    "build-react": "(echo \"Building React...\") && webpack --config ./webpack.config.ts --mode development",
    "watch-react": "(echo \"Watching React...\") && webpack --config ./webpack.config.ts --watch --mode development",
    "prebuild-engine": "echo \"\\x1b[103m\\x1b[97m[ Building Engine ]\\x1b[0m\"",
    "build-engine": "(echo \"Building Engine...\") && cmake-js -d ../engine compile",
    "prebuild-all": "echo \"\\x1b[104m\\x1b[97m[ Building All ]\\x1b[0m\"",
    "build-all": "yarn build-engine && yarn build-react",
    "prelaunch-electron": "echo \"\\x1b[105m\\x1b[97m[ Launching Electron ]\\x1b[0m\"",
    "launch-electron": "concurrently \"electron src/electron.js\" \"yarn watch-react\"",
    "prestart": "yarn clean && yarn build-all",
    "start": "yarn launch-electron",
    "prestart-editor": "yarn build-react",
    "start-editor": "yarn launch-electron",
    "reload": "yarn build-react"
  },
  "author": "Deepak Ramalingam",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mui/material": "^5.2.3",
    "@mui/styles": "^5.2.3",
    "@types/material-ui": "^0.21.12",
    "@types/react-tap-event-plugin": "^0.0.31",
    "concurrently": "^6.4.0",
    "electron-reload": "^2.0.0-alpha.1",
    "fs": "^0.0.1-security",
    "material-ui-image": "^3.3.2",
    "node-addon-api": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@types/electron": "^1.6.10",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.5.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-loader": "^8.2.3",
    "cmake-js": "^6.2.1",
    "electron": "^16.0.1",
    "eslint": "^8.4.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "node-loader": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
