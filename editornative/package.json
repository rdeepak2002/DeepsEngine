{
  "name": "editornative",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "preclean": "echo \"\\x1b[101m\\x1b[97m[ Cleaning ]\\x1b[0m\"",
    "clean": "(cmake-js -d ../engine clean) && (rm -rf ./dist || true) && (rm ../engine/frame.png || true)",
    "prebuild-engine": "echo \"\\x1b[103m\\x1b[97m[ Building Engine ]\\x1b[0m\"",
    "build-engine": "cmake-js -d ../engine compile",
    "prestart": "yarn clean && yarn build-engine",
    "start": "yarn expo-electron start",
    "start-editor": "yarn expo-electron start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "cmake-js": "^6.3.0",
